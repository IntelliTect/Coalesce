{
  "name": "coalesce-vue-vuetify3",
  "version": "0.2.0",
  "main": "./dist/coalesce-vue-vuetify.js",
  "module": "./dist/coalesce-vue-vuetify.js",
  "styles": "./dist/coalesce-vue-vuetify.css",
  "types": "./dist/index.d.ts",
  "type": "module",
  "sideEffects": [
    "*.css"
  ],
  "exports": {
    ".": "./dist/coalesce-vue-vuetify.js",
    "./lib/build": "./dist/build.js",
    "./build": "./dist/build.js",
    "./styles": "./dist/coalesce-vue-vuetify.css",
    "./*": "./*"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vue-tsc --noEmit && vite build && npm run buildts",
    "buildts": "tsc ./src/build.ts --declaration --outDir ./dist",
    "build-local-deps": "cd ../coalesce-vue && npm ci && npm run build",
    "test": "vitest",
    "coverage": "vitest --coverage"
  },
  "dependencies": {
    "date-fns": "^2.28.0",
    "date-fns-tz": "^1.3.3",
    "tslib": "^2.3.1"
  },
  "peerDependencies": {
    "coalesce-vue": "../coalesce-vue",
    "vue": "^3.2.34",
    "vuetify": "^3.1.4"
  },
  "optionalDependencies": {
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vitest/coverage-c8": "^0.28.5",
    "@vue/eslint-config-typescript": "11.0.2",
    "@vue/test-utils": "^2.2.10",
    "autoprefixer": "^10.4.8",
    "axios": "^0.26.1",
    "coalesce-vue": "../coalesce-vue",
    "eslint": "8.28.0",
    "eslint-plugin-vue": "9.7.0",
    "jsdom": "^20.0.3",
    "postcss-scss": "^4.0.4",
    "sass": "1.32.13",
    "typescript": "4.9.3",
    "unplugin-vue-components": "0.22.11",
    "vite": "4.1.1",
    "vite-plugin-dts": "^1.7.1",
    "vite-plugin-vuetify": "1.0.2",
    "vitest": "0.28.5",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6",
    "vue-tsc": "1.1.0",
    "vuetify": "3.1.5"
  },
  "postcss": {
    "//": "This is to silence warnings when consuming directly from Coalesce.Web.Vue3. Otherwise, not needed.",
    "plugins": {
      "autoprefixer": {}
    }
  }
}
