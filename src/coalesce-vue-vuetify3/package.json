{
  "name": "coalesce-vue-vuetify3",
  "version": "0.1.0-local",
  "description": "Vuetify 3 components for IntelliTect's Coalesce framework.",
  "repository": "https://github.com/IntelliTect/Coalesce",
  "author": "IntelliTect",
  "license": "Apache-2.0",
  "private": false,
  "main": "./dist/coalesce-vue-vuetify.js",
  "module": "./dist/coalesce-vue-vuetify.js",
  "styles": "./dist/coalesce-vue-vuetify.css",
  "types": "./dist/index.d.ts",
  "type": "module",
  "sideEffects": [
    "*.css"
  ],
  "exports": {
    ".": {
      "style": "./dist/coalesce-vue-vuetify.css",
      "types": "./dist/index.d.ts",
      "default": "./dist/coalesce-vue-vuetify3.js"
    },
    "./build": {
      "types": "./dist/build.d.ts",
      "default": "./dist/build.js"
    },
    "./*": "./dist/*",
    "./styles": "./dist/coalesce-vue-vuetify.css",
    "./styles.css": "./dist/coalesce-vue-vuetify.css"
  },
  "typesVersions": {
    "*": {
      "*": [
        "*",
        "dist/*"
      ]
    }
  },
  "files": [
    "README.md",
    "dist"
  ],
  "scripts": {
    "build": "npm run build-local-deps && vite build && vue-tsc --noEmit && vue-tsc -p ./tsconfig.build-types.json && npm run buildts",
    "buildts": "tsc -p ./tsconfig.build-utils.json",
    "build-local-deps": "cd ../coalesce-vue && npm run build",
    "test": "vitest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "coverage": "vitest --coverage"
  },
  "dependencies": {
    "date-fns": "^3.0.0",
    "date-fns-tz": "^3.0.0",
    "tslib": "^2.6.2",
    "vue": "^3.5.4"
  },
  "peerDependencies": {
    "coalesce-vue": "0.1.0-local",
    "vuetify": "^3.7.1"
  },
  "optionalDependencies": {
    "vue-router": "^4.4.1"
  },
  "devDependencies": {
    "@types/node": "^20.14.11",
    "@vitejs/plugin-vue-jsx": "^5.1.1",
    "coalesce-vue": "../coalesce-vue",
    "sass": "^1.69.7",
    "vue-router": "^4.4.1",
    "vuetify": "3.10.3"
  }
}
