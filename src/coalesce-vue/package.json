{
  "name": "coalesce-vue",
  "version": "0.1.0-local",
  "description": "Coalesce core scripts for Vue.js 2 and 3.",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js"
    },
    "./lib/*": {
      "types": "./lib/*.d.ts",
      "import": "./lib/*.js"
    }
  },
  "repository": "https://github.com/IntelliTect/Coalesce",
  "author": "IntelliTect",
  "license": "Apache-2.0",
  "private": false,
  "scripts": {
    "build": "tsc --noEmit && tsc -p ./tsconfig.build.esm.json",
    "prepack": "npm run build",
    "test": "vitest",
    "coverage": "vitest --coverage"
  },
  "files": [
    "lib",
    "src",
    "README.md"
  ],
  "dependencies": {
    "@types/lodash-es": "^4.17.3",
    "axios": "^1.3.4",
    "date-fns": "^3.0.0",
    "date-fns-tz": "^3.0.0",
    "lodash-es": "^4.17.15",
    "magic-string": "^0.30.5",
    "tslib": "^2.6.2",
    "vue": "^3.5.4"
  },
  "optionalDependencies": {
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@babel/types": "^7.18.4",
    "vue-router": "4.5.0"
  }
}
