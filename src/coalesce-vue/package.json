{
  "name": "coalesce-vue",
  "version": "0.1.0-local",
  "description": "Coalesce core scripts for Vue.js 2 and 3.",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/cjs/index.js"
    },
    "./lib/*": {
      "types": "./lib/*.d.ts",
      "import": "./lib/*.js",
      "require": "./lib/cjs/*.js"
    }
  },
  "repository": "https://github.com/IntelliTect/Coalesce",
  "author": "IntelliTect",
  "license": "Apache-2.0",
  "private": false,
  "scripts": {
    "build": "tsc --noEmit && tsc -p ./tsconfig.build.esm.json && tsc -p ./tsconfig.build.cjs.json",
    "prepack": "npm run build",
    "test": "vitest",
    "coverage": "vitest --coverage"
  },
  "files": [
    "lib",
    "src",
    "README.md"
  ],
  "dependencies": {
    "@types/lodash-es": "^4.17.3",
    "axios": "^1.3.4",
    "date-fns": "^3.0.0",
    "date-fns-tz": "^3.0.0",
    "lodash-es": "^4.17.15",
    "magic-string": "^0.30.5",
    "tslib": "^2.6.2"
  },
  "peerDependencies": {
    "vue": "^2.7.8 || ^3.2.0"
  },
  "optionalDependencies": {
    "vue-router": "^3.0.0 || ^4.0.0"
  },
  "devDependencies": {
    "@babel/types": "^7.18.4",
    "@vitest/coverage-v8": "^1.1.1",
    "@vue/test-utils": "1.3.6",
    "cross-blob": "^1.2.1",
    "jsdom": "^23.0.1",
    "lodash": "^4.17.15",
    "typescript": "5.6.3",
    "vite": "^5.4.6",
    "vitest": "^1.1.1",
    "vue": "2.7.8",
    "vue-template-compiler": "2.7.8",
    "vue-router": "3.5.4"
  }
}
