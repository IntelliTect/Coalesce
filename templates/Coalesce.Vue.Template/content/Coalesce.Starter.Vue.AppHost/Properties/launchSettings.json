{
  "$schema": "https://json.schemastore.org/launchsettings.json",
  "profiles": {
    "https": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": true,

      // Disables injection of MSFT's undesirable JS that force-refreshes the page when C# files are changed.
      "hotReloadEnabled": false,

      "applicationUrl": "https://localhost:5003",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "DOTNET_ENVIRONMENT": "Development",
        "DOTNET_DASHBOARD_OTLP_ENDPOINT_URL": "https://localhost:5004",
        "DOTNET_RESOURCE_SERVICE_ENDPOINT_URL": "https://localhost:5005"
      }
    }
  }
}
